{
  "index_patterns": [
    "%%PREFIX%%-tracardi-log-*-*"
  ],
  "template": {
    "settings": {
      "number_of_shards": 5,
      "number_of_replicas": 1
    },
    "mappings": {
      "dynamic": false,
      "properties": {
        "message": {
          "type": "keyword"
        },
        "logger": {
          "type": "keyword"
        },
        "file": {
          "type": "keyword"
        },
        "line": {
          "type": "integer"
        },
        "level": {
          "type": "integer"
        }
      }
    }
  }
}